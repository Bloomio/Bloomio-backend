{
  "name": "bloomio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "mintest": "jest --forceExit --runInBand",
    "test": "eslint . && jest --coverage --forceExit --runInBand",
    "testJ": "jest",
    "test-nolint": "jest --coverage --forceExit --runInBand",
    "test-console": "jest --coverage",
    "dbon": "mkdir -p ./db && mongod -v --dbpath ./db",
    "dboff": "killall mongod",
    "build": "babel src -d build",
    "start": "node index.js",
    "coverage-badge": "coverage-badger -r coverage/clover.xml -d coverage/",
    "coverage": "npm test -- --coverage && npm run coverage-badge",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__test__/lib/test.env.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bloomio/backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bloomio/backend/issues"
  },
  "homepage": "https://github.com/Bloomio/backend#readme",
  "devDependencies": {
    "aws-sdk-mock": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "jest": "^22.4.3",
    "superagent": "^3.8.3",
    "winston": "^3.0.0-rc5"
  },
  "dependencies": {
    "aws-sdk": "^2.236.1",
    "babel-jest": "^22.4.3",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "crypto": "^1.0.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "fs-extra": "^6.0.1",
    "http-errors": "^1.6.3",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.1",
    "mongoose": "^5.0.17",
    "multer": "^1.3.0",
    "twilio": "^3.17.0"
  }
